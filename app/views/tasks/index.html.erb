<h1>My tasks</h1>
<ul>
<% @tasks.each do |task| %>
  <li><%= task.completed%>
  <%= link_to task.title, task_path(task) %>
  <%= link_to "Delete", destroy_path(task), data: {turbo_method: :delete, turbo_confirm:'Are you sure?'} %>
  <%= link_to "Edit a task", edit_path(task) %>
</li>
<% end %>
</ul>
<%= link_to "+ Add a new task", tasks_new_path %>
